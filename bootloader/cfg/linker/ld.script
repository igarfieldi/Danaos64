/* Designate entry point of the bootloader (located in bootloader.s) */
ENTRY(_start)
OUTPUT_FORMAT(elf32-i386)

SECTIONS
{
	. = 0x7C00;
	/* Bootloader code */
	.text :
	{
		*(.bootsector)
		*(.text)
		*(.data)
		*(.eh_frame)
		*(.rodata)
		*(COMMON)
		*(.bss)
		. = ALIGN(512);
	}
	
    /DISCARD/ :
    {
    	*(.comment)
    }
}
